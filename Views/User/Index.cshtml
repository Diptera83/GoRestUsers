@{
}
@model IEnumerable<UserInfo>


    <h3>Tüm Kullanıcılar</h3>
<table class="table">
    <thead>
        <tr>
            <th>Id:</th>
            <th>Email:</th>
            <th>Gender</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var myUser in Model)
        {
            <tr>
                <td>@myUser.id</td>
                <td>@myUser.Email</td>
                <td>@myUser.Gender</td>
                <td>@myUser.status</td>

            </tr>
        }
    </tbody>
</table>